{
    "compilerOptions": {
        "allowJs": false,
        "declaration": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "lib": [
            "es2020"
        ],
        "module": "commonjs",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "outDir": "./dist",
        "strict": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "target": "es2020"
    },
    "include": [
        "./agent/*.ts"
    ]
}
